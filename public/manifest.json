{
  "manifest_version": 3,
  "name": "Tracemark",
  "version": "1.0",
  "description": "A Chrome extension that lets you draw over any webpage and copy the result as an image.",
  "action": {
    "default_title": "Open Tracemark"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "./src/background/background.js",
    "type": "module"
  }
}
